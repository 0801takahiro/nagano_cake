<h2>商品一覧</h2>

<table>
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
<% @customers.each do |customer| %>
  <tbody>
    <td><%= customer.id %></td>
    <td><%= link_to "customer.name", edit_admin_customer(customer.id) %></td>
    <td><%= customer.address %></td>
    <td><%= customer.is_active %></td>
  </tbody>
</table>
<% end %>
<div class="pagination">
    <p>1</p>
    <p>2</p>
    <p>3</p>
    <p>Next></p>
　　<p>Last>></p>
</div>