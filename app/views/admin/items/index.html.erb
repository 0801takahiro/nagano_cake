<h2>商品一覧</h2>

<table>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜き価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
<% @items.each do |item| %>
  <tbody>
    <td><%= item.id %></td>
    <td><%= link_to "item.name", edit_admin_item(item.id) %></td>
    <td><%= item.price %></td>
    <td><%= item.name %></td>
    <td><%= item.is_active %></td>
  </tbody>
</table>
<% end %>
<div class="pagination">
    <p>1</p>
    <p>2</p>
    <p>3</p>
    <p>Next></p>
　　<p>Last>></p>
</div>